{
	"name": "@wwwhana/n8n-nodes-qbittorrent",
	"version": "0.6.0",
	"description": "qBittorrent node for n8n",
	"keywords": [
		"n8n-community-node-package"
	],
	"license": "MIT",
	"homepage": "https://github.com/wwwhana/n8n-nodes-qbittorrent",
	"author": {
		"name": "lexia",
		"email": "limlexia@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/wwwhana/n8n-nodes-qbittorrent.git"
	},
	"publishConfig": {
		"registry": "https://npm.pkg.github.com"
	},
	"main": "index.js",
	"scripts": {
		"build": "rimraf dist && tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes --write",
		"lint": "eslint nodes package.json",
		"lintfix": "eslint nodes package.json --fix",
		"prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes package.json",
		"release": "release-it",
		"test": "vitest"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/QBittorrentApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/QBittorrent/QBittorrent.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
